{
  "name": "get-title-ts",
  "version": "1.0.0",
  "description": "URLを入力するとサイトタイトルを取得するCLI",
  "main": "./lib/main.js",
  "scripts": {
    "start": "npm run cli",
    "cli": "node ./bin/cli.js https://github.com",
    "test": "jest -c ./jest.config.json --detectOpenHandles",
    "build": "npm run clean && tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf lib",
    "format": "prettier --write src/**/*.ts src/**/*.tsx && npm run lint:fix"
  },
  "keywords": [
    "cli"
  ],
  "author": "Himenon",
  "license": "MIT",
  "bin": {
    "get-title": "./lib/cli.js"
  },
  "devDependencies": {
    "@types/jest": "^23.3.4",
    "@types/puppeteer": "^1.9.0",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "1.14.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "puppeteer": "^1.9.0"
  },
  "engines": {
    "node": "<=9.8.0",
    "npm": "<=6.4"
  }
}
